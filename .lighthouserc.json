{
    "ci": {
        "collect": {
            "url": [
                "https://meoumi.dev/",
                "https://meoumi.dev/logs"
            ],
            "numberOfRuns": 1
        },
        "assert": {
            "preset": "lighthouse:recommended",
            "assertions": {
                "categories:seo": [
                    "error",
                    {
                        "minScore": 0.9
                    }
                ],
                "unminified-css": "off",
                "unused-javascript": [
                    "warn",
                    {
                        "maxLength": 120000
                    }
                ]
            }
        }
    }
}